#!/usr/bin/awk

#
# See ../README.md
#

#
# Run as: awk -f ch-1.awk
#

BEGIN {
    m = split ("1600 2000", start_years)
    n = split ("004     009     015     020     026 " \
               "032     037     043     048     054 " \
               "060     065     071     076     082 " \
               "088     093     099                 " \
               "        105     111     116     122 " \
               "128     133     139     144     150 " \
               "156     161     167     172     178 " \
               "184     189     195                 " \
               "        201     207     212     218 " \
               "224     229     235     240     246 " \
               "252     257     263     268     274 " \
               "280     285     291     296         " \
               "                303     308     314 " \
               "320     325     331     336     342 " \
               "348     353     359     364     370 " \
               "376     381     387     392     398", long_year_offsets)

    for (j = 1; j <= m; j ++) {
        for (i = 1; i <= n; i ++) {
            year = start_years [j] + long_year_offsets [i]
            if (1900 <= year && year <= 2100) {
                print year
            }
        }
    }
}
