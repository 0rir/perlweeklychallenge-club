say "Open rooms: \n", (1..^500).grep({ 
    is-open( $_ ) 
}).join(",");

sub is-open( $i )
{
    my $is-open = !($i %% 2);
    $is-open = !$is-open if $i %% $_ for 3, 4;
    $is-open;
}

