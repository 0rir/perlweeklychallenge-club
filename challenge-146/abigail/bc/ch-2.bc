#
# Run as: bc ch-2.bc < input-file
#

while (1) {
    a = read (); if (a == 0) {break}
    b = read (); if (b == 0) {break}
    for (i = 1; i <= 2; i ++) {
        if (a < b) {
            b = b - a
        } else {
            a = a - b
        }
        if (a == 0 || b == 0) {
            break
        }
        print a, "/", b, " "
    }
    "
"
}

